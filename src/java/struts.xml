<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
        <package name="default" extends="struts-default">       
            <action name="ebaySellerLogin" class="ebaySellerLoginAction">
                    <result name="success">/jsp/index.jsp</result>
            </action>
            <action name="getToken" class="getTokenAction"></action>

            <action name="getSoldItems" class="getSoldItemsAction">
                <result name="success">/jsp/soldItems.jsp</result>
            </action>
            <action name="completeSale" class="completeSaleAction"></action>
            <action name="getCompleteSaleForm" class="getCompleteSaleFormAction">
                <result name="success">/jsp/completeSaleForm.jsp</result>
            </action>
            <action name="autoFeedback" class="autoFeedbackAction"></action>


            <action name="showAllItems" class="showAllItemsAction">
                    <result name="success">/jsp/allItemsList.jsp</result>
            </action>
            <action name="showActiveItems" class="showActiveItemsAction">
                    <result name="success">/jsp/activeItemsList.jsp</result>
            </action>
            <action name="showMyMessages" class="showMyMessagesAction">
                    <result name="success">/jsp/myMessagesList.jsp</result>
            </action>
            <action name="showItem" class="showItemAction">
                    <result name="success">/jsp/itemDetail.jsp</result>
            </action>
            <action name="addItem" class="addItemAction">
                    <result name="success">/jsp/addItemResult.jsp</result>
            </action>
        </package>
</struts>
